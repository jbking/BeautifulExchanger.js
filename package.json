{
  "name": "BeautifulExchanger",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbking/BeautifulExchanger.js.git"
  },
  "private": true,
  "devDependencies": {
    "jsdoc": "~3.3.0-beta1"
  },
  "engine": {
    "node": ">= 0.10",
    "npm": ">= 2.1"
  },
  "scripts": {
    "docs" : "jsdoc -r src/ -d docs/",
    "docs:clean" : "rm -rf docs",
    "docs-inner" : "jsdoc -p -r src/ -d docs-inner/",
    "docs-inner:clean" : "rm -rf docs-inner/",
    "rst2md" : "pandoc README.rst -s -o README.md"
  }
}
